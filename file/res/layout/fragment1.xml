<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:layout_marginTop="25dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:layout_marginEnd="20dp"
                android:gravity="center"
                android:src="@drawable/button_background"
                android:orientation="vertical">

                <ImageView
                    android:id="@+id/btnHunter"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:onClick="btnHunter"
                    android:src="@drawable/hunterx"
                    android:contentDescription="@string/Sinopsis_hunterx"
                    tools:ignore="UsingOnClickInXml"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:layout_marginEnd="20dp"
                android:orientation="vertical">

                <ImageView
                    android:id="@+id/btnbleach"
                    android:src="@drawable/bleach"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:onClick="btnBleach"
                    tools:ignore="UsingOnClickInXml"/>


            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:orientation="vertical">

                <ImageView
                    android:id="@+id/btnnanatsu"
                    android:src="@drawable/nanatsu"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:onClick="btnNanatsu"
                    tools:ignore="UsingOnClickInXml"/>

            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:layout_marginEnd="20dp"
                android:gravity="center"
                android:src="@drawable/button_background"
                android:orientation="vertical">

                <ImageView
                    android:id="@+id/btnonepiece"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/onepiece"
                    android:background="@drawable/button_background"
                    android:onClick="btnOnepiece"
                    tools:ignore="UsingOnClickInXml" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:layout_marginEnd="20dp"
                android:orientation="vertical">

                <ImageView
                    android:id="@+id/btndigimon"
                    android:src="@drawable/digimon"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:onClick="btnDigimon"
                    tools:ignore="UsingOnClickInXml"/>


            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:orientation="vertical">

                <ImageView
                    android:id="@+id/btnsamurai"
                    android:src="@drawable/samurai"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:onClick="btnsamurai"
                    tools:ignore="UsingOnClickInXml"/>

            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:layout_marginEnd="20dp"
                android:gravity="center"
                android:src="@drawable/button_background"
                android:orientation="vertical">

                <ImageView
                    android:id="@+id/btnnaruto"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/naruto"
                    android:background="@drawable/button_background"
                    android:onClick="btnnaruto"
                    tools:ignore="UsingOnClickInXml" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:layout_marginEnd="20dp"
                android:orientation="vertical">

                <ImageView
                    android:id="@+id/btndororo"
                    android:src="@drawable/dororo"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:onClick="btndororo"
                    tools:ignore="UsingOnClickInXml"/>


            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:orientation="vertical">

                <ImageView
                    android:id="@+id/btnspyx"
                    android:src="@drawable/spyx"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:onClick="btnspyx"
                    tools:ignore="UsingOnClickInXml"/>

            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:layout_marginEnd="20dp"
                android:gravity="center"
                android:src="@drawable/button_background"
                android:orientation="vertical">

                <ImageView
                    android:id="@+id/btnrezero"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/button_background"
                    android:onClick="btnrezero"
                    android:src="@drawable/rezero"
                    tools:ignore="UsingOnClickInXml" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:layout_marginEnd="20dp"
                android:orientation="vertical">

                <ImageView
                    android:id="@+id/btnsword"
                    android:src="@drawable/sword"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:onClick="btnsword"
                    tools:ignore="UsingOnClickInXml"/>


            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:orientation="vertical">

                <ImageView
                    android:id="@+id/btntate"
                    android:src="@drawable/tate"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:onClick="btntate"
                    tools:ignore="UsingOnClickInXml"/>

            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:layout_marginEnd="20dp"
                android:gravity="center"
                android:src="@drawable/button_background"
                android:orientation="vertical">

                <ImageView
                    android:id="@+id/btnslime"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/slime"
                    android:background="@drawable/button_background"
                    android:onClick="btnslime"
                    tools:ignore="UsingOnClickInXml" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:layout_marginEnd="20dp"
                android:orientation="vertical">

                <ImageView
                    android:id="@+id/btnoverlord"
                    android:src="@drawable/overlord"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:onClick="btnoverlord"
                    tools:ignore="UsingOnClickInXml"/>


            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:orientation="vertical">

                <ImageView
                    android:id="@+id/btnnogame"
                    android:src="@drawable/nogame"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:onClick="btnnogame"
                    tools:ignore="UsingOnClickInXml"/>

            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:layout_marginEnd="20dp"
                android:gravity="center"
                android:src="@drawable/button_background"
                android:orientation="vertical">

                <ImageView
                    android:id="@+id/btntsikimichi"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/tsukimichi"
                    android:background="@drawable/button_background"
                    android:onClick="btntsukimichi"
                    tools:ignore="UsingOnClickInXml" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:layout_marginEnd="20dp"
                android:orientation="vertical">

                <ImageView
                    android:id="@+id/btnisekaicheat"
                    android:src="@drawable/isekaicheat"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:onClick="btnisekaicheat"
                    tools:ignore="UsingOnClickInXml"/>


            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:orientation="vertical">

                <ImageView
                    android:id="@+id/btndeadmarch"
                    android:src="@drawable/deadmarch"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:onClick="btndeadmarch"
                    tools:ignore="UsingOnClickInXml"/>

            </LinearLayout>
        </LinearLayout>


    </LinearLayout>


</androidx.core.widget.NestedScrollView>